<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="/style.css"/>
  </head>
  <body>
    <ul>
      <% posts.forEach((post) => { %>
      <li>
        <a href=<%=`/api/posts/${post.id}`%>><%=post.title%></a> 
        <p><%=post.id%></p>
      </li>
      <% })%>
    </ul> 
  </body>
</html>

